<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            
            show_content();
            $('.datepicker').datepicker();
            $('.timepicker').timepicker();
            $.validate();
            $('#top_nav_living_room').addClass('active_yellow');
        }
        loadMallDataCached(renderAll); 
    })
    
    
</script>