<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            
            show_content();
            get_instagram("//dixieliving.mallmaverick.com/api/v2/dixieliving/social.json", 100, 'standard_resolution', render_feed)
            $('#top_nav_living_room').addClass('active_yellow')
        }
        loadMallDataCached(renderAll); 
    })
    
    function render_feed(data){
        $('#instagram_feed').html(data);
    }
</script>