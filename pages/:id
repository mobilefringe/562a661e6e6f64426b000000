<div id="full_width" class="fluid_row hidden_now">
    <div class="row">
        <div class="col-md-12 contact_rows">
            <div class="margin_20"></div>
            <h3 class="hours_header" id="page_title"></h3>
            <div id="page_content"></div>
        </div>
    </div>
</div>
<div class="margin_40"></div>
<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var pathArray = window.location.pathname.split( '/' );
            var slug = pathArray[pathArray.length-1];
            prefix = get_prefix();
            var pages_json = prefix + "/pages/" + slug + ".json"
            $.getJSON(pages_json).done(function(data) {
                if (slug.includes('dixieoutlet-covid-19-update')) {
                    $("#full_width").show();
                    $('#page_title').html(data.title);
                    $('#page_content').html(data.body);
                } else {
                    $('#mm_page_title').html(data.title);
                    $('#mm_page_content').html(data.body);
                    $("#regular").show();
                }
            }).fail(function(jqXHR) {
                if (jqXHR.status == 404) {
                    $("#404_msg").fadeIn("fast");
                }
            }); 
            show_content();
        }
        loadMallDataCached(renderAll); 
    })
</script>